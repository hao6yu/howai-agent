# HaoGPT Environment Configuration
# Copy this file to .env and fill in your values

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_CHAT_MODEL=gpt-4o
OPENAI_CHAT_MINI_MODEL=gpt-4o-mini

# Optional: secure proxy endpoint (recommended for production)
# If set, the app sends OpenAI requests to this backend URL instead of OpenAI directly.
OPENAI_PROXY_BASE_URL=
OPENAI_PROXY_TOKEN=

# ElevenLabs (optional - for voice features)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Google APIs (for location/maps features)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Supabase (optional - for cloud sync)
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
