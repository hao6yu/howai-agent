# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Models
OPENAI_CHAT_MODEL=gpt-4.1
OPENAI_CHAT_MINI_MODEL=gpt-4.1-mini
OPENAI_REALTIME_MODEL=gpt-4o-realtime-preview
OPENAI_REASONING_MODEL=o3

# ElevenLabs API Key
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Google Custom Search API credentials (FREE - 100 searches/day)
# Get API key from: https://console.cloud.google.com/apis/credentials
# Create search engine at: https://programmablesearchengine.google.com/
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CSE_ID=your_custom_search_engine_id_here

# Google Places API Key (for local discovery feature)
# Get API key from: https://console.cloud.google.com/apis/credentials
# Enable Places API and Maps SDK at: https://console.cloud.google.com/apis/library
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Google Maps API Key (for map display - can be same as GOOGLE_PLACES_API_KEY)
# Enable Maps SDK for Android, Maps SDK for iOS at: https://console.cloud.google.com/apis/library
# This key is used by both Android (via build.gradle) and iOS (via LocalConfig.xcconfig)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here 